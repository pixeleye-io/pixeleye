{
  "name": "@pixeleye/storybook",
  "description": "Storybook integration for Pixeleye",
  "private": false,
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "pnpm rimraf ./dist && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts"
  },
  "main": "src/index.ts",
  "typings": "src/index.ts",
  "files": [
    "README.md",
    "bin",
    "LICENSE"
  ],
  "peerDependencies": {
    "puppeteer": "^20 || ^21",
    "puppeteer-core": "^20 || ^21"
  },
  "dependencies": {
    "@pixeleye/booth": "workspace:*",
    "@pixeleye/puppeteer": "workspace:*",
    "@storybook/addons": "^7.4.6",
    "puppeteer": "^21.3.5",
    "rrweb-snapshot": "2.0.0-alpha.11",
    "undici": "^5.25.2"
  },
  "devDependencies": {
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/js-sdk": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/node": "20.7.0",
    "execa": "^8.0.1",
    "get-port": "^7.0.0",
    "rimraf": "^5.0.5",
    "typescript": "5.2.2"
  }
}

{
  "name": "@pixeleye/cli",
  "description": "Pixeleye CLI",
  "version": "0.0.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "upload-images": "node ./dist/index.js upload --url=http://localhost:5000/v1 ./test-images",
    "run:storybook": "node ./dist/index.js storybook https://localhost:6006",
    "build": "tsup",
    "dev": "pnpm run build -- --watch"
  },
  "devDependencies": {
    "@pixeleye/api": "workspace:*",
    "@pixeleye/booth": "workspace:*",
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/node": "20.7.0",
    "@types/require-relative": "^0.8.0",
    "esbuild": "^0.19.3",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "@pixeleye/js-sdk": "workspace:*",
    "@pixeleye/storybook": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "config": "^3.3.9",
    "dedent": "^1.5.1",
    "env-ci": "^10.0.0",
    "execa": "^8.0.1",
    "jsdom": "^22.1.0",
    "ora": "^7.0.1",
    "require-relative": "^0.8.7",
    "ts-node": "^10.9.1"
  }
}

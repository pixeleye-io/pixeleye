{
  "name": "@pixeleye/cli-config",
  "version": "0.6.8",
  "private": false,
  "license": "AGPL-3.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "rimraf ./bin && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "rimraf": "^5.0.7"
  },
  "dependencies": {
    "@pixeleye/cli-devices": "workspace:*",
    "@pixeleye/cli-env": "workspace:*",
    "fast-glob": "^3.3.2",
    "jiti": "^1.21.0",
    "sucrase": "^3.35.0"
  }
}
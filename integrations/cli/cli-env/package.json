{
  "name": "@pixeleye/cli-env",
  "version": "0.6.0",
  "private": false,
  "license": "AGPL-3.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "rimraf ./bin && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@pixeleye/cli-logger": "workspace:*",
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/node": "20.12.7",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "env-ci": "^11.0.0",
    "execa": "^8.0.1"
  }
}
{
  "name": "@pixeleye/cli-asset-server",
  "version": "0.0.0",
  "private": false,
  "license": "AGPL-3.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "rimraf ./bin && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/polka": "^0.5.7",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@pixeleye/cli-dom": "workspace:*",
    "polka": "^0.5.2",
    "get-port": "^7.0.0"
  }
}
{
  "name": "@pixeleye/cypress",
  "description": "Cypress integration for Pixeleye",
  "private": false,
  "version": "0.6.0",
  "license": "AGPL-3.0",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "pnpm rimraf ./dist && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "README.md",
    "dist",
    "LICENSE"
  ],
  "peerDependencies": {
    "cypress": "^13"
  },
  "dependencies": {
    "@pixeleye/cli-booth": "workspace:*",
    "@pixeleye/cli-devices": "workspace:*",
    "rrweb-snapshot": "2.0.0-alpha.13",
    "undici": "^6.18.1"
  },
  "devDependencies": {
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/node": "20.12.12",
    "cypress": "^13.10.0",
    "rimraf": "^5.0.7",
    "typescript": "5.4.5"
  }
}
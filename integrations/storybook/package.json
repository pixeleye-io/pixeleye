{
  "name": "@pixeleye/storybook",
  "private": false,
  "version": "0.6.1",
  "scripts": {
    "lint": "eslint",
    "types": "tsc --noEmit",
    "build": "pnpm rimraf ./dist && node ./build.mjs & tsc --emitDeclarationOnly --outDir ./dist"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "README.md",
    "dist",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "dependencies": {
    "@pixeleye/cli-devices": "workspace:*",
    "@pixeleye/cli-logger": "workspace:*",
    "@pixeleye/playwright": "workspace:*",
    "@storybook/addons": "^7.6.17",
    "playwright-core": "^1.43.0",
    "rrweb-snapshot": "2.0.0-alpha.13"
  },
  "devDependencies": {
    "@pixeleye/esbuild": "workspace:*",
    "@pixeleye/eslint-config": "workspace:*",
    "@pixeleye/tsconfig": "workspace:*",
    "@types/node": "20.12.7",
    "rimraf": "^5.0.5",
    "typescript": "5.4.5"
  }
}
{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["KRATOS_PUBLIC_URL", "ORY_SDK_URL", "KRATOS_BROWSER_URL", "GITHUB_DOCS_TOKEN"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:types": {},
    "lint": {},
    "types": {},
    "test": {},
    "test:api": {},
    "start": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
